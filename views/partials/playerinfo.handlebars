<div class="col-8">
    <div class="collapse multi-collapse" id="{{character_name}}">
        <div class="card bg-dark" style="width: 18rem;">
            <div class="row">
                <div class="col-7">
                    <img class="img-thumbnail bg-success" alt="...">
                    <h5 class="card-title text-center text-white">{{character_name}}</h5>
                    <h6 class="card-title text-center text-white">{{player_race}}</h6>
                    <h6 class="card-title text-center text-white">{{player_class}}</h6>
                    <h6 class="card-title text-center text-white">{{level}}</h6>
                </div>
                <div class="col-5">
                    <ul class="list-group list-group-flush">
                        {{> playerlist}}
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>